{
	"name": "@anephenix/auth",
	"module": "dist/auth.esm.js",
	"version": "0.0.2",
	"scripts": {
		"analyze": "size-limit --why",
		"build": "tsc --project tsconfig.json",
		"createDatabase": "node scripts/createDatabase.js",
		"cover": "cross-env NODE_ENV=test npx vitest test --run --coverage",
		"cucumber": "NODE_ENV=test npx cucumber-js --no-strict",
		"lint": "npx @biomejs/biome format --write",
		"migrate": "npx knex migrate:latest",
		"prepare": "husky",
		"prepare-patch-release": "npm run update-changelog && git add CHANGELOG.md && git commit -m \"Updated changelog\" && npm version patch",
		"publish-patch-release": "npm run prepare-patch-release && git push origin main && git push --tags",
		"size": "size-limit",
		"start": "node server.js",
		"update-changelog": "node --experimental-strip-types scripts/update-changelog.ts",
		"test": "cross-env NODE_ENV=test npx vitest test --run",
		"watch": "tsc --project tsconfig.json --watch"
	},
	"keywords": [
		"fastify",
		"objection",
		"authentication"
	],
	"author": "Paul Jensen <paul@anephenix.com>",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"type": "module",
	"engines": {
		"node": ">=12"
	},
	"dependencies": {
		"bcrypt": "^6.0.0",
		"bluebird": "^3.7.2",
		"knex": "^3.1.0",
		"objection": "^3.1.3",
		"objection-password": "^3.0.0",
		"pg": "^8.16.3",
		"redis": "^5.6.1",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@cucumber/cucumber": "^12.1.0",
		"@size-limit/esbuild": "^11.2.0",
		"@size-limit/esbuild-why": "^11.2.0",
		"@size-limit/preset-small-lib": "^11.2.0",
		"@vitest/coverage-v8": "^3.2.4",
		"cross-env": "^10.0.0",
		"husky": "^9.1.7",
		"nyc": "^17.1.0",
		"size-limit": "^11.2.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"size-limit": [
		{
			"path": "dist/*.js",
			"limit": "10 KB"
		}
	]
}
